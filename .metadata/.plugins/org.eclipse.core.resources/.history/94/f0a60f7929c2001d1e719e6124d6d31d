<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ page session="false" contentType="text/html; charset=utf-8" %>
<html>
<head>
	<title>1to10</title>
</head>
<body>
	<%
		int sum = 0;
		for(int i = 0; i <= 10; i++){
			sum += i;
		}
	%>
	
	<p>1부터 10까지의 합 : <%= sum %></p>
</body>
</html>